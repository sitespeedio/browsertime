{
  "name": "browsertime",
  "description": "Get performance metrics from your web page using Browsertime.",
  "version": "24.0.1",
  "bin": "./bin/browsertime.js",
  "type": "module",
  "types": "./types/scripting.d.ts",
  "dependencies": {
    "@devicefarmer/adbkit": "3.3.8",
    "@sitespeed.io/chromedriver": "132.0.6834-83",
    "@sitespeed.io/edgedriver": "132.0.2957-115",
    "@sitespeed.io/geckodriver": "0.35.0-1",
    "@sitespeed.io/log": "0.2.4",
    "@sitespeed.io/throttle": "5.0.1",
    "@sitespeed.io/tracium": "0.3.3",
    "chrome-har": "1.0.1",
    "chrome-remote-interface": "0.33.2",
    "execa": "9.5.2",
    "fast-stats": "0.0.7",
    "ff-test-bidi-har-export": "0.0.17",
    "lodash.merge": "4.6.2",
    "selenium-webdriver": "4.28.1",
    "yargs": "17.7.2"
  },
  "optionalDependencies": {
    "@jimp/custom": "0.22.12",
    "@jimp/png": "0.22.12",
    "@jimp/jpeg": "0.22.12",
    "@jimp/plugin-resize": "0.22.12",
    "@jimp/plugin-scale": "0.22.12",
    "usb-power-profiling": "1.5.0"
  },
  "devDependencies": {
    "@types/selenium-webdriver": "4.1.25",
    "ava": "6.2.0",
    "clean-jsdoc-theme": "4.3.0",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-unicorn": "56.0.1",
    "jsdoc": "4.0.4",
    "prettier": "3.4.2",
    "serve": "14.2.4",
    "serve-handler": "6.1.6",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE",
    "README.md",
    "bin",
    "browserscripts",
    "browsersupport",
    "visualmetrics/visualmetrics-portable.py",
    "visualmetrics/visualmetrics.py",
    "browsertime.png",
    "index.js",
    "lib",
    "package.json",
    "vendor",
    "types"
  ],
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./types/scripting.d.ts"
    }
  },
  "scripts": {
    "test": "ava",
    "start-server": "serve test/data/html/",
    "stop-server": "sudo pkill serve",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tsc": "tsc",
    "jsdoc": "jsdoc --configure jsdoc/jsdoc.json",
    "prepublishOnly": "npm run lint && npm run tsc"
  },
  "author": "Peter Hedenskog",
  "contributors": [
    {
      "name": "Gregory Mierzwinski"
    },
    {
      "name": "Tobias Lidskog"
    },
    {
      "name": "Jonathan Lee"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sitespeedio/browsertime.git"
  },
  "homepage": "https://www.sitespeed.io/documentation/browsertime/",
  "license": "MIT",
  "ava": {
    "files": [
      "test/**/*",
      "!test/util",
      "!test/data"
    ],
    "concurrency": 1,
    "failFast": true,
    "require": [
      "./test/util/setup.js"
    ],
    "nodeArguments": [
      "--trace-deprecation"
    ]
  }
}
